---
import ObfuscatedData, { type StyleableComponent } from './ObfuscatedData.astro';
import { Type } from '../Type';

interface Props extends StyleableComponent {
  email: string
  text?: string
}

const { email, text, ...other } = Astro.props;
---

<ObfuscatedData data={email} text={text} type={Type.EMAIL} {...other} />
